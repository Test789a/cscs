    //cntry= 0;
    //sales_month_ar = {};
    define ar_cntr type i size 2;

    for(i = 1; i <= Size(queryResult)-1; i++)
    {
        
        if(i >= Size(queryResult))
        {
            arrayProsla[i-1]= 0; 
            sales_month_ar[i-1]= 0; 
